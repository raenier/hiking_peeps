<%= turbo_frame_tag "profile" do %>
  <%= link_to edit_profile_path(@profile) do %>
    <%= image_tag 'edit.svg', alt: 'Edit' %> Update
  <% end if current_user == @profile.user %>

  <%= content_tag :div, @profile.gender, class: 'gender' %>
  <%= content_tag :div, @profile.bday, class: 'bday' %>
  <%= content_tag :div, @profile.address, class: 'address' %>
  <%= content_tag :div, "\"#{@profile.description}\"", class: 'description' %>
<% end %>
